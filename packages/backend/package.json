{
  "name": "@workspace/backend",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "main": "./convex/_generated/api.js",
  "types": "./convex/_generated/api.d.ts",
  "exports": {
    ".": {
      "types": "./convex/_generated/api.d.ts",
      "import": "./convex/_generated/api.js",
      "require": "./convex/_generated/api.js"
    },
    "./api": {
      "types": "./convex/_generated/api.d.ts",
      "import": "./convex/_generated/api.js",
      "require": "./convex/_generated/api.js"
    },
    "./server": {
      "types": "./convex/_generated/server.d.ts",
      "import": "./convex/_generated/server.js",
      "require": "./convex/_generated/server.js"
    },
    "./convex/*": "./convex/*"
  },
  "scripts": {
    "dev": "convex dev",
    "setup": "convex dev --until-success"
  },
  "devDependencies": {
    "@workspace/eslint-config": "workspace:*",
    "@workspace/tsconfig": "workspace:*",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "convex": "^1.25.4"
  },
  "files": [
    "convex/**/*"
  ]
}
